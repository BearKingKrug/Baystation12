{{if !data.claw }}
	<b>Claw is not synced. Please sync claw.</b><br>
	{{:helper.link('Sync Claw', 'angle-double-down', { 'find_claw' : '1' }, null, null)}} <br>
{{else data.maint}}
	<b>Claw is in maint mode.</b><br>
{{else}}
	<div style="float:left;width:45%;">
		<fieldset style="min-height:180px;background-color: #202020;">
			<legend style="text-align:center">Slimes</legend>
			<div>
	    		{{for data.slimes}}
					<div class='item'>
						{{:helper.link(value.key1, 'biohazard', { 'slime_click' : value.key2 }, data.grabbed && data.grabbed == 1 ? 'disabled' : null)}}<br>
					</div>
				{{/for}}
			</div>
		</fieldset>
	</div>

	<fieldset style="min-height:180px;background-color: #202020;width:225px;float:right">
		<legend style="text-align:center">Manual control</legend>
		<div class='item'>
			<div class='item'>
				{{:helper.link('', 'triangle-1-nw', { 'move' : 9 },null,null)}}
				{{:helper.link('', 'triangle-1-n', { 'move' : 1 },null,null)}}
				{{:helper.link('', 'triangle-1-ne', { 'move' : 5 },null,null)}} <div style="float:right">{{:helper.link('Grab', 'biohazard', { 'grab' : '1' }, data.grabbed && data.grabbed == 1 ? 'disabled' : null)}}</div>
			</div>
			<div class='item'>
				{{:helper.link('', 'triangle-1-w', { 'move' : 8 },null,null)}}
				{{:helper.link('', 'circle-close', { 'brake' : 1 },null,null)}}
				{{:helper.link('', 'triangle-1-e', { 'move' : 4 },null,null)}} <div style="float:right">{{:helper.link('Release', 'biohazard', { 'release' : '1' }, data.grabbed ? null : 'disabled')}}</div>
				</div>
			<div class='item'>
				{{:helper.link('', 'triangle-1-sw', { 'move' : 10 },null,null)}}
				{{:helper.link('', 'triangle-1-s', { 'move' : 2 },null,null)}}
				{{:helper.link('', 'triangle-1-se', { 'move' : 6 },null,null)}} <div style="float:right">{{:helper.link('Spray', 'biohazard', { 'spray' : '1' }, null)}}</div>
			</div>
		</div>
	</fieldset>
	<br>
	<div style="float:left;width:45%;">
		<fieldset style="min-height:180px;background-color: #202020;">
			<legend style="text-align:center">Beacons</legend>
			<div>
	    		{{for data.beacons}}
					<div class='item'>
						{{:helper.link(value.key1, 'biohazard', { 'beacon_click' : value.key2 }, null)}}<br>
					</div>
				{{/for}}
			</div>
		</fieldset>
	</div>
	{{for data.pathd}}
		<div class='item'>
			{{:value.x}} | {{:value.y}} | {{:value.z}}
		</div>
	{{/for}}
{{/if}}
